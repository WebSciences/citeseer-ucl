<!DOCTYPE dwr PUBLIC
  "-//GetAhead Limited//DTD Direct Web Remoting 1.0//EN"
  "http://www.getahead.ltd.uk/dwr/dwr10.dtd">
  
<dwr>
  <allow>
    <create creator="spring" javascript="AccountValidatorJS">
      <param name="beanName" value="accountValidator"/>
      <include method="getInputFieldValidationMessage"/>
    </create>
    <create creator="spring" javascript="GetContextJS">
      <param name="beanName" value="citeContextUtil"/>
      <include method="getContext"/>
    </create>
    <create creator="spring" javascript="MetadataCartJS">
      <param name="beanName" value="metadataCartDWR"/>
      <include method="addToCart"/>
      <include method="removeFromCart"/>
    </create>
  </allow>
</dwr>
